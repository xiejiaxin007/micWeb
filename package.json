    "author": "yanshijing <yanshijing@julive.com>",
    "private": true,
    "scripts": {
        "dev": "vue-cli-service serve",
        "dll": "webpack -p --progress --config ./webpack.dll.conf.js",
        "unit": "jest --config test/unit/jest.conf.js",
        "unit:coverage": "jest --config test/unit/jest.conf.js --coverage",
        "unit:watch": "jest --config test/unit/jest.conf.js --watchAll",
        "e2e": "node test/e2e/runner.js",
        "test": "npm run unit && npm run e2e",
        "build-production": "npm run unit && vue-cli-service build",
        "build-dev": "npm run unit && vue-cli-service build --mode jltest",
        "build-sandbox": "npm run unit && vue-cli-service build --mode sandbox",
        "analyz": "NODE_ENV=production npm_config_report=true vue-cli-service build"
    },
